// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Experience {
  id          Int     @default(autoincrement()) @id
  name        String
  company     String?
  personal    Boolean @default(false)
  from        String?
  to          String?
  description String
  website     String?

}
